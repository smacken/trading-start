{
    "name": "trading-start",
    "version": "1.0.0",
    "description": "init trading platform",
    "main": "index.js",
    "scripts": {
        "start": "node index.js --file --holdings",
        "build": "tsc",
        "watch:build": "tsc --watch",
        "lint": "tslint -c tslint.json",
        "transactions": "node index.js --file --transactions",
        "commsec": "node index.js --holdings",
        "test": "jest --coverage"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
        "coverageDirectory": "./coverage/",
        "collectCoverage": true
    },
    "author": "scott mackenzie",
    "repository": {
        "type": "git",
        "url": "git://github.com/smacken/trading-start.git"
    },
    "license": "ISC",
    "dependencies": {
        "cacheman-file": "^0.2.1",
        "commsec": "^0.2.0",
        "csv-parse": "^4.8.2",
        "csv-stringify": "^5.3.4",
        "fs-extra-promise": "^1.0.1",
        "mocha": "^6.2.2",
        "prompt": "^1.0.0",
        "stream-transform": "^2.0.1"
    },
    "devDependencies": {
        "@types/jest": "^24.0.23",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.16",
        "jest": "^24.9.0",
        "ts-jest": "^24.2.0",
        "typescript": "^3.7.3"
    }
}
